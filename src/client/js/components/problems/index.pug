.ui.text.container
  table.ui.celled.table.selectable.pointer
    thead
      tr
        th ID
        th Problem Name
    tbody
      tr( v-for="prob of problems" v-link="{ name: 'problem', params: { id: prob._id }}",
        :class="{ disabled: !prob.visible }" )
        td.force-pointer {{ prob._id }}
        td.force-pointer {{ prob.name }}

